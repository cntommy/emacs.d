#+begin_src python :results file :var tbl_data=b-o-n tbl_data2=b-s-n tbl_data3=b-s-s :exports results
  import numpy, matplotlib
  matplotlib.use('Agg')
  import matplotlib.pyplot as plt
  x = [22]+[i for i in range(25,51,5)]
  y2 = tbl_data2[0][1:]
  y3 = tbl_data3[0][1:]
  y = tbl_data[0][1:]
  plt.plot(x,y3,'y',label="ours:SCCL+statistic+simcse")
  plt.plot(x,y2,'b',label="ours:SCCL+statistic")
  plt.plot(x,y,'k',label="ours:SCCL otigin baseline")

  plt.scatter(26,88.7,label='1st')
  plt.scatter(35,79,label='2nd')
  plt.scatter(42,78,label='3rd')
  plt.scatter(22,64,label='ours')
  plt.legend(loc="best")
  #  fig.tight_layout()
  plt.savefig('1.png')
  return('1.png')
#+end_src

